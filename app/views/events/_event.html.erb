<tr id="<%=e.id%>">
	<td><%=e.contact%></td>
	<td><%=link_to "#{e.job_prospect.company}", job_prospect_path(e.job_prospect), :method => 'get'%></td>
	<td><%=date(e.conversation_date.month, e.conversation_date.day, e.conversation_date.year)%></td>
	<td><%= time(e.conversation_time.hour, e.conversation_time.min)%></td>
	<td><%=e.conversation_type%></td>
	<td><%=e.contact_info%></td>
	<td><%=notes(e.notes)%></td>
	<td><%=link_to "Edit", edit_job_prospect_event_path(e.job_prospect, e)%></td>
	<td><%=link_to "Delete", job_prospect_event_path(e.job_prospect, e), :confirm => "Are you sure you want to delete this event?", :method => 'delete', :remote => true%></td>
</tr>